<div class="page-header">
  <h1>Cohorts</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Org</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cohorts.each do |cohort| %>
      <tr>
        <td><%= link_to cohort.name, cohort %></td>
        <td><%= cohort.org %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cohort', new_cohort_path %>
